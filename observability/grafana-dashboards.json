{
  "child_safety_dashboard": {
    "dashboard": {
      "id": null,
      "title": "AI Teddy Bear - Child Safety Monitoring",
      "tags": ["ai-teddy", "child-safety", "critical"],
      "style": "dark",
      "timezone": "browser",
      "editable": true,
      "hideControls": false,
      "graphTooltip": 1,
      "time": {
        "from": "now-24h",
        "to": "now"
      },
      "timepicker": {
        "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
        "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
      },
      "refresh": "30s",
      "panels": [
        {
          "id": 1,
          "title": "üî¥ Critical Safety Violations (Real-time)",
          "type": "stat",
          "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
          "targets": [
            {
              "expr": "sum(increase(child_safety_violations_total{severity=\"critical\"}[5m]))",
              "legendFormat": "Critical Violations (5m)",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {"mode": "thresholds"},
              "thresholds": {
                "steps": [
                  {"color": "green", "value": null},
                  {"color": "yellow", "value": 1},
                  {"color": "red", "value": 3}
                ]
              },
              "unit": "short",
              "min": 0
            }
          },
          "options": {
            "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
            "orientation": "auto",
            "textMode": "auto",
            "colorMode": "background",
            "graphMode": "area",
            "justifyMode": "auto"
          }
        },
        {
          "id": 2,
          "title": "üö® Emergency Response Time",
          "type": "gauge",
          "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
          "targets": [
            {
              "expr": "histogram_quantile(0.95, rate(emergency_protocol_activations_duration_bucket[5m])) / 1000",
              "legendFormat": "P95 Response Time (seconds)",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {"mode": "thresholds"},
              "thresholds": {
                "steps": [
                  {"color": "green", "value": null},
                  {"color": "yellow", "value": 10},
                  {"color": "red", "value": 15}
                ]
              },
              "unit": "s",
              "min": 0,
              "max": 30
            }
          },
          "options": {
            "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
            "orientation": "auto",
            "showThresholdLabels": false,
            "showThresholdMarkers": true
          }
        },
        {
          "id": 3,
          "title": "üìä COPPA Compliance Score",
          "type": "stat",
          "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
          "targets": [
            {
              "expr": "coppa_compliance_score * 100",
              "legendFormat": "COPPA Compliance %",
              "refId": "A"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {"mode": "thresholds"},
              "thresholds": {
                "steps": [
                  {"color": "red", "value": null},
                  {"color": "yellow", "value": 95},
                  {"color": "green", "value": 99}
                ]
              },
              "unit": "percent",
              "min": 90,
              "max": 100
            }
          },
          "options": {
            "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
            "orientation": "auto",
            "textMode": "auto",
            "colorMode": "background",
            "graphMode": "area",
            "justifyMode": "auto"
          }
        },
        {
          "id": 4,
          "title": "üõ°Ô∏è Content Toxicity Distribution",
          "type": "heatmap",
          "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
          "targets": [
            {
              "expr": "increase(content_toxicity_score_bucket[5m])",
