              "legendFormat": "P95 Age Appropriateness",
              "refId": "B"
            }
          ],
          "fieldConfig": {
            "defaults": {
              "color": {"mode": "palette-classic"},
              "unit": "short",
              "min": 0,
              "max": 1,
